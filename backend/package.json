{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "MUHAMMED HASHIM",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "prisma generate --schema=src/models/schema.prisma && ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prisma:generate": "prisma generate --schema=src/models/schema.prisma",
    "migration:create": "prisma migrate dev --create-only --schema=src/models/schema.prisma",
    "migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express-async-handler": "^1.2.0",
    "mongoose": "^8.14.1",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.16",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "globals": "^16.1.0",
    "morgan": "^1.10.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
